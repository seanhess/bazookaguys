


#board
  background url(/images/bg.jpg) no-repeat
  width 800px
  height 500px
  position relative

  .player
    background url(/images/player1.png) no-repeat center bottom
    position absolute
    width 50px
    height 50px

#identify

  .avatar
    float left
    width 40px
    height 40px
    margin 5px
    border solid 3px #EEE

  .avatar.player1
    background url(/images/player1.png) no-repeat center bottom

  .name
    clear both

  .players
    .player
      clear both

